{
    "compilerOptions": {
        "target": "ES2015",
        "module": "CommonJS",
        "strict": true,
        "noImplicitAny": false,
        "allowJs": true,
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "baseUrl": "./src/",
        "paths": {
            "#/*": [
                "*"
            ]
        },
    },
    "include": [
        "./src/**/*"
    ]
}